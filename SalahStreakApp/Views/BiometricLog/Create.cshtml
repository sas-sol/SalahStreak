     ;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>BiometricLog</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ParticipantId" class="control-label"></label>
                <input asp-for="ParticipantId" class="form-control" />
                <span asp-validation-for="ParticipantId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ParticipantId_int" class="control-label"></label>
                <select asp-for="ParticipantId_int" class ="form-control" asp-items="ViewBag.ParticipantId_int"></select>
